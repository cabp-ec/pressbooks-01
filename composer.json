{
  "name": "cabp/pressbooks-01",
  "description": "Coding challenge for Pressbooks.",
  "keywords": [
    "php",
    "framework",
    "laravel",
    "lumen",
    "Carlos",
    "Bucheli",
    "Pressbooks"
  ],
  "license": "proprietary",
  "type": "project",
  "authors": [
    {
      "name": "Carlos Bucheli",
      "email": "cabp@carlos-bucheli.com",
      "homepage": "http://www.carlos-bucheli.com",
      "role": "Owner"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "laravel/lumen-framework": "5.8.0",
    "vlucas/phpdotenv": "3.4"
  },
  "require-dev": {
    "behat/behat": "^3.8",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test/tests/"
    ]
  },
  "scripts": {
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
